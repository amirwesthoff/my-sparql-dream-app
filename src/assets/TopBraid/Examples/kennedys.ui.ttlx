kennedys:AnthonyShriver
  rdfs:comment "Anthony Paul Kennedy Shriver (born July 20, 1965, in Boston, Massachusetts) is an American activist for people with intellectual disabilities. In 1989, he founded Best Buddies International, an international organization that helps people with intellectual disabilities to find employment and social opportunities." ;
.
kennedys:EdwardKennedy
  rdfs:comment "Edward Moore \"Ted\" Kennedy (February 22, 1932 â€“ August 25, 2009) was a United States Senator from Massachusetts and a member of the Democratic Party. He was the second most senior member of the Senate when he died and was the fourth-longest-serving senator in United States history, having served there for almost 47 years. As the most prominent living member of the Kennedy family for many years, he was also the last surviving son of Joseph P. Kennedy, Sr.; the youngest brother of President John F. Kennedy and Senator Robert F. Kennedy, both victims of assassination; and the father of Congressman Patrick J. Kennedy." ;
.
kennedys:Person
  search:facetedSearch kennedys:PersonFacetedSearch ;
  ui:instanceView """
<swa:ResourceSummary arg:defaultImage=\"{= &lt;http://topbraid.org/examples/images/Person-Placeholder.png&gt; }\" arg:resource=\"{= ?this }\" ui:id=\"summary\"/>
"""^^ui:Literal ;
.
kennedys:PersonFacetedSearch
  rdf:type search:Search ;
  search:facet [
      rdf:type search:PropertyFacet ;
      search:facetIndex 0 ;
      search:property kennedys:gender ;
    ] ;
  search:facet [
      rdf:type search:PropertyFacet ;
      search:facetIndex 1 ;
      search:property kennedys:almaMater ;
    ] ;
  search:facet [
      rdf:type search:PropertyFacet ;
      search:facetIndex 2 ;
      search:property kennedys:profession ;
    ] ;
  search:facet [
      rdf:type search:PropertyFacet ;
      search:facetIndex 3 ;
      search:property kennedys:lastName ;
    ] ;
  search:facet [
      rdf:type search:PropertyFacet ;
      search:facetIndex 4 ;
      search:property kennedys:birthYear ;
    ] ;
  search:rootType kennedys:Person ;
  rdfs:comment "An example Search that shall be used whenever someone uses a faceted search engine on kennedys:Person." ;
  rdfs:label "Person faceted search" ;
.
kennedys:photo
  rdfs:subPropertyOf swa:thumbnail ;
.
<http://topbraid.org/examples/kennedys.ui>
  rdf:type owl:Ontology ;
  rdfs:comment "Defines various SWP customizations for the Kennedys ontology, including a faceted search and a customized summary view." ;
  owl:imports <http://topbraid.org/examples/kennedys> ;
  owl:imports <http://topbraid.org/search> ;
  owl:imports <http://topbraid.org/swa> ;
  owl:versionInfo "0.1.0" ;
.
